{
  "name": "@herolabid/litetable-core",
  "version": "0.2.2",
  "description": "Core logic for LiteTable - Framework-agnostic table library with virtual scrolling, export, and zero dependencies",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "table",
    "datatable",
    "datagrid",
    "core",
    "headless",
    "typescript",
    "virtual-scroll",
    "export",
    "xlsx",
    "csv",
    "server-side",
    "pagination",
    "sorting",
    "filtering",
    "zero-dependencies",
    "lightweight"
  ],
  "author": "Irfan Arsyad <irfan@herostack.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/herolabid/LiteTable.js.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/herolabid/LiteTable.js#readme",
  "bugs": {
    "url": "https://github.com/herolabid/LiteTable.js/issues"
  },
  "funding": {
    "type": "buymeacoffee",
    "url": "https://www.buymeacoffee.com/herostack"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^3.7.2"
  },
  "sideEffects": false
}
